/**index.wxss**/
/*头像内容  */
.wel-info {
  display: flex;
  flex-direction: column;
  align-items: center;
}
/*头像图片  */
.wel-img {
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
}
/*昵称  */
.wel-nickname {
  color: #aaa;
}
/*底部 内容  */
.wel-bottom {
  margin-top: 170rpx;
}
/*欢迎字体  */
.wel-font {
  color:#10b4be;
  font-size: 30px;
}
/*进入  */
.wel-login {
  margin-top: 60rpx;
  flex-direction:row;
  display: flex;
}
/*底部 红色字  */
.wel-color {
  color:#209b2b;
}